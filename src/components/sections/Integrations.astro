/* 8 columns on desktop, 6 on mobile â€” uniform grid, checkerboard fill */
const cols = 8;
---

<section id="integrations" class={cn('section-divider py-12 lg:py-36', className)} aria-labelledby="integrations-heading">
  <div class="mx-auto max-w-6xl px-4 sm:px-6 lg:px-8">
    <div class="mb-12 text-center lg:mb-16" data-animate>
      <h2 id="integrations-heading" class="text-3xl font-semibold leading-[0.95] tracking-tight sm:text-4xl lg:text-5xl">Works with 500+ platforms</h2>
      <p class="mx-auto mt-5 max-w-2xl text-base text-dark/60 sm:text-lg">Connect Midsphere to the tools you already use. No complex setup required.</p>
    </div>

    <div class="logo-grid" data-animate data-delay="1">
      {platforms.map((p, i) => {
        const row = Math.floor(i / cols);
        const col = i % cols;
        const isShaded = (row + col) % 2 === 1;
        return (
          <div class:list={['logo-cell', { 'logo-cell--shaded': isShaded }]} title={p.name}>
            <svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 24 24" fill={p.color} class="logo-icon" aria-label={p.name} role="img" style={p.darkColor ? `--dark-fill:${p.darkColor}` : undefined}>
              <path d={p.d} />
            </svg>
          </div>
        );
      })}
    </div>
  </div>
</section>

<style>
  .logo-grid {
    display: grid;
    grid-template-columns: repeat(8, 1fr);
    border-top: 1px solid rgba(0, 0, 0, 0.05);
    border-left: 1px solid rgba(0, 0, 0, 0.05);
  }

  .logo-cell {
    aspect-ratio: 1;
    display: flex;
    align-items: center;
    justify-content: center;
    border-right: 1px solid rgba(0, 0, 0, 0.05);
    border-bottom: 1px solid rgba(0, 0, 0, 0.05);
    transition: background-color 0.2s ease, transform 0.2s ease;
  }

  .logo-cell--shaded {
    background-color: rgba(0, 0, 0, 0.025);
  }

  .logo-cell:hover {
    background-color: rgba(0, 0, 0, 0.06);
    transform: scale(1.08);
    z-index: 1;
  }

  :global([data-theme="dark"]) .logo-grid {
    border-color: rgba(255, 255, 255, 0.05);
  }

  :global([data-theme="dark"]) .logo-cell {
    border-color: rgba(255, 255, 255, 0.05);
  }

  :global([data-theme="dark"]) .logo-cell--shaded {
    background-color: rgba(255, 255, 255, 0.03);
  }

  :global([data-theme="dark"]) .logo-cell:hover {
    background-color: rgba(255, 255, 255, 0.07);
  }

  .logo-icon {
    width: 30%;
    height: 30%;
    transition: transform 0.2s ease;
  }

  :global([data-theme="dark"]) .logo-icon[style*="--dark-fill"] {
    fill: var(--dark-fill);
  }

  @media (max-width: 639px) {
    .logo-grid {
      grid-template-columns: repeat(6, 1fr);
    }
  }
</style>
